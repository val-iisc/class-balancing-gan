{
  "data_processing":{
    "dataset_name": "cifar10",
    "data_path": "./data/cifar10",
    "img_size": 32,
    "num_classes": 10,
    "batch_size4prcsing": 256,
    "chunk_size": 500,
    "compression": false
  },

  "train": {
    "model": {
      "architecture": "dcgan",
      "conditional_strategy": "ContraGAN",
      "pos_collected_numerator": true,
      "hypersphere_dim": 512,
      "nonlinear_embed": false,
      "normalize_embed": true,
      "g_spectral_norm": false,
      "d_spectral_norm": true,
      "activation_fn": "ReLU",
      "attention": false,
      "attention_after_nth_gen_block": "N/A",
      "attention_after_nth_dis_block": "N/A",
      "z_dim": 128,
      "shared_dim": "N/A",
      "g_conv_dim": "N/A",
      "d_conv_dim": "N/A",
      "G_depth": "N/A",
      "D_depth": "N/A"
    },

    "optimization": {
      "optimizer": "Adam",
      "batch_size": 64,
      "accumulation_steps": 1,
      "d_lr": 0.0002,
      "g_lr": 0.0002,
      "momentum": "N/A",
      "nesterov": "N/A",
      "alpha": "N/A",
      "beta1": 0.5,
      "beta2": 0.999,
      "g_steps_per_iter": 1,
      "d_steps_per_iter": 1,
      "total_step": 200000
    },

    "loss_function": {
      "adv_loss": "hinge",

      "contrastive_lambda": 1.0,
      "margin": 1.0, 
      "tempering_type": "discrete",
      "tempering_step": 1,
      "start_temperature": 1.0,
      "end_temperature": 2.0,

      "weight_clipping_for_dis": false,
      "weight_clipping_bound": "N/A",

      "gradient_penalty_for_dis": false,
      "gradient_penelty_lambda": "N/A",

      "consistency_reg": false,
      "consistency_lambda": "N/A",

      "bcr": false,
      "real_lambda": "N/A",
      "fake_lambda": "N/A",

      "zcr": false,
      "gen_lambda": "N/A",
      "dis_lambda": "N/A",
      "sigma_noise": "N/A"
    },

    "initialization":{
      "g_init": "ortho",
      "d_init": "ortho"
    },

    "training_and_sampling_setting":{
      "random_flip_preprocessing": false,
      "diff_aug": false,

      "ada": false,
      "ada_target": "N/A",
      "ada_length": "N/A",

      "prior": "gaussian",
      "truncated_factor": 1,

      "latent_op": false,
      "latent_op_rate":"N/A",
      "latent_op_step":"N/A",
      "latent_op_step4eval":"N/A",
      "latent_op_alpha":"N/A",
      "latent_op_beta":"N/A",
      "latent_norm_reg_weight":"N/A",

      "ema": true,
      "ema_decay": 0.9999,
      "ema_start": 20000
    }
  }
}
